{
    "name": "borja/trovimap-php-client",
    "description": "Simple client to interact with trovimap evaluation API",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "phpfastcache/phpfastcache": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0|~3.8.0"
    },
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Borja",
            "email": "borja@propertista.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Trovimap\\":"src/",
            "Test\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trovimap\\TrovimapServiceProvider"
            ]
        }
    }
}
